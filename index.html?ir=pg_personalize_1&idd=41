            	                                            for ($tipo = 0; $tipo < 3; $tipo++) {
	                                                if($tipo==0){ $array = $canvas['dinamic']; }
	                                                if($tipo==1){ $array = $canvas['text']; }
	                                                if($tipo==2){ $array = $canvas['fixed']; }
	
	                                                $.each($array, function($key, $value) {
	                                                    if($value['class'] == 'CA_MATERIAL_verso' && 'materias_1_black_localhost4000_zz143b36e2eb.jpg' != 'undefined'){
	                                                        $value['move'] = 1;
	                                                        $value['move_pos_x'] = undefined;
	                                                        $value['move_pos_y'] = undefined;
	                                                        if($value['class'] == 'CA_UPLOAD_IMAGENS_1' || $value['class'] == 'CA_UPLOAD_IMAGENS_2' || $value['class'] == 'CA_UPLOAD_IMAGENS_3'){
	                                                            canvas_inserir_img_upload('materias_1_black_localhost4000_zz143b36e2eb.jpg', $value['class'].replace('CA_UPLOAD_IMAGENS_', ''))
	                                                        }
	                                                        setTimeout(function(){
	                                                            $value['tamanho_pos_x'] = undefined;
	                                                            $value['tamanho_pos_y'] = undefined;
	                                                            $value['move'] = 0;
	                                                        }, .5);
	
	                                                        if($tipo==1){
	                                                            $('.'+$value['class'].replace('_verso', '')+' input').val('materias_1_black_localhost4000_zz143b36e2eb.jpg');
	                                                            if(''){ $('.'+$value['class'].replace('_verso', '')+' .frente').trigger('click'); }
	                                                            if(''){  $('.'+$value['class'].replace('_verso', '')+' .verso').trigger('click'); }
	                                                        } else {
	                                                            $('.'+$value['class'].replace('_verso', '')+' li[foto="materias_1_black_localhost4000_zz143b36e2eb.jpg"]').trigger('click');
	                                                        }
	                                                    }
	                                                });
	                                            }
            
            canvas_loop();
        }());
    </script>

