            <script>
                $(document).ready(function() {
                    $(".CA_CATEGORIAS_ELEMENTOS").on("change", function() {
                        if($(this).val() == 0){
                            $(".categorias_").show();
                        } else {
                            $(".categorias_").hide();
                            $(".categorias_"+$(this).val()).show();
                        }
                    })
                });
