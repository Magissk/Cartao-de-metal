        <script>
            $BANNER_CARTAO = 26;
            function banner_cartao($x){
                $x = parseInt($x/30);
                $x = $x > 1 ? $x : 1;
                $(".BANNER_CARTAO img").hide();
                if($x <= $BANNER_CARTAO){
                    $(".BANNER_CARTAO img.img_"+$x).show();
                }
            }
            $(window).scroll(function(){
                banner_cartao($(window).scrollTop());
            });

        </script>

